{
    "name": "jmj/jushuitan-sdk",
    "description": "Jushuitan ERP SDK,用于PHP对接聚水潭开放平台相关接口",
    "keywords": ["jushuitan","sdk","聚水潭"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Smilefish2/jushuitan-sdk",
    "authors": [
        {
            "name": "JMJones",
            "email": "yang.jmj@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "ext-openssl": "*",
        "ext-fileinfo": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0"
